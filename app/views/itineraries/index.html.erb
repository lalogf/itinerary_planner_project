                </ul>
                <a href="/itineraries/<%=it.id%>/waypoints/new" class="pull-right pull-down btn btn-xs btn-info"> Add Waypoint</a>

              </div>
            </div>
          </div>
        </div>
        <%end %>
      </div>

      <div class="col-md-2 col-md-offset-1">
        <div class="panel panel-info sidebar">
          <div class="panel-heading">

            <h3 class="panel-title">Friends for travel</h3>
            <a href="/travelers/new" class=" btn btn-warning btn-xs"> Add partner</a>
          </div>
          <div class="panel-body ">

            <% @friends.each do |f| %>
            <span class="glyphicon glyphicon-user" aria-hidden="true"></span>&nbsp;&nbsp;&nbsp;  <a href="/travelers/<%= f.id%>"><%= f.firstname %> <%= f.lastname%></a> <br>

            <% end %>

            
          </div>
        </div>
      </div>

